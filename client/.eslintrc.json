{
  "extends": [
    "next/core-web-vitals",
    "next/typescript",
    "next/core-web-vitals",
    "prettier"
  ],
  "plugins": ["unused-imports"],
  "rules": {
    "@typescript-eslint/no-unused-vars": [
      "warn",
      {
        "vars": "all",
        "varsIgnorePattern": "^_",
        "args": "all",
        "argsIgnorePattern": "^_",
        "caughtErrors": "all",
        "caughtErrorsIgnorePattern": "^_"
      }
    ],
    "unused-imports/no-unused-imports": "error"
  }
}
